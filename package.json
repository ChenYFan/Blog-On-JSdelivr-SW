{
	"name": "clientworker",
	"version": "4.0.0",
	"packageManager": "pnpm@8.15.4",
	"author": "ChenYFan",
	"contributors": [
		"Ray <i@mk1.io> (@so1ve)"
	],
	"type": "module",
	"description": "A simple, globally hijacked, easily customizable, Service Worker-based front-end worker.",
	"keywords": [
		"ClientWorker",
		"ServiceWorker"
	],
	"homepage": "https://github.com/ChenYFan/ClientWorker#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ChenYFan/ClientWorker.git"
	},
	"bugs": {
		"url": "https://github.com/ChenYFan/ClientWorker/issues"
	},
	"license": "LGPL-3.0",
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./with-yaml": {
			"types": "./dist/with-yaml.d.ts",
			"import": "./dist/with-yaml.js"
		}
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./dist/index.d.ts"
			]
		}
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "rimraf dist && tsup && nr build:minify",
		"build:minify": "tsup --minify",
		"lint": "eslint . && prettier . --check",
		"lint:fix": "eslint . --fix && prettier . --write",
		"prepublishOnly": "nr build",
		"release": "bumpp --commit --push --tag && pnpm publish",
		"start": "tsx src/index.ts",
		"test": "vitest",
		"typecheck": "tsc --noEmit",
		"watch": "nr build --watch"
	},
	"devDependencies": {
		"@antfu/ni": "^0.21.8",
		"@so1ve/eslint-config": "^1.4.0",
		"@so1ve/prettier-config": "^1.4.0",
		"@types/node": "^20.8.3",
		"bumpp": "^9.2.0",
		"eslint": "^8.51.0",
		"js-yaml": "^4.1.0",
		"prettier": "^3.0.3",
		"rimraf": "^5.0.5",
		"tiny-request-router": "^1.2.2",
		"tsup": "^8.0.2",
		"tsx": "^3.13.0",
		"type-fest": "^4.10.3",
		"typescript": "^5.2.2",
		"vite": "^4.4.11",
		"vitest": "^0.34.6"
	}
}
